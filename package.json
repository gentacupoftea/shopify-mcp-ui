  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "react-scripts build",
    "build:analyze": "GENERATE_SOURCEMAP=true react-scripts build && npm run analyze",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html build/bundle-analysis.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:a11y": "cypress run --spec \"cypress/e2e/a11y/**/*.cy.js\"",
    "test:a11y": "react-scripts test --testPathPattern=src/tests/a11y",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "npm run build:prod && ./deploy.sh staging",
    "deploy:prod": "npm run build:prod && ./deploy.sh production"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
